{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "ts-node ./tests/pagination.test.ts",
    "prisma": "prisma",
    "migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset --force",
    "seed": "ts-node prisma/seed.ts",
    "setup": "npm run migrate && npm run seed",
    "admin:update-email": "node scripts/admin-tools.js update-email",
    "admin:create": "node scripts/admin-tools.js create-admin"
  },
  "dependencies": {
    "@prisma/client": "^5.12.0",
    "@types/bcryptjs": "^2.4.6",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.13",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^20.19.13",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^2.0.12",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "prisma": "^5.12.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
